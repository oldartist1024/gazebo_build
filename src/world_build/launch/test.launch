<launch>

  <!-- 1. 使用仿真时间 -->
  <param name="use_sim_time" value="true"/>

  <!-- 2. 启动 Gazebo 并加载 world -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name"
         value="$(find world_build)/world/test.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
  </include>

  <node pkg="gazebo_ros" type="spawn_model" name="spawn_usv"
      args="-sdf
            -file $(find world_build)/models/test.sdf
            -model usv_boat"/>


</launch>